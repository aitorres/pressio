<section class="menu">
	<div class="avatar well">
	<%if @foto %>
		<%= image_tag @foto %>
	<%else %>
		<%= image_tag "desconocido.png" %>
	<%end%>
	</div>
	<%if usuario_signed_in? %>
		<p class="text-success text-center">Bienvenido, <%= @usuario.nombre + " " + @usuario.apellido %></p>
	<%else %>
		<h2>Bienvenido, desconocido</h2>
	<%end%>
	<hr>
	<p class="text-warning text-center">Editar Perfil</p>
	<p class="text-danger text-center">Cerrar Sesion</p>


</section>